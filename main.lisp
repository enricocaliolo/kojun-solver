(defun kojun-board ()
  '((5 0 2 0 2 0 3 1 3 1)
    (0 4 0 1 0 5 0 5 0 4)
    (7 5 1 7 0 0 3 1 3 0)
    (0 4 0 0 0 0 0 0 0 3)
    (2 0 3 4 0 2 0 0 4 0)
    (5 0 2 0 6 0 0 0 0 0)
    (0 1 3 0 1 0 0 4 0 3)
    (6 7 0 3 0 1 4 0 0 1)
    (4 0 3 0 4 0 0 0 0 3)
    (0 1 0 2 0 6 2 0 2 1)))

(defun kojun-regions ()
  '((01 02 02 02 03 03 03 03 04 04)
    (01 01 01 02 05 05 06 06 04 06)
    (07 07 01 05 05 08 09 06 06 06)
    (07 07 05 05 10 08 09 09 09 11)
    (07 07 07 05 10 10 12 11 11 11)
    (13 13 14 14 14 10 15 15 16 16)
    (13 13 13 14 14 18 19 20 16 16)
    (17 17 13 14 18 18 19 20 21 21)
    (17 17 22 22 22 22 19 20 20 23)
    (17 17 17 22 22 22 19 19 23 23)))

(defun print-kojun-board (board)
  (dolist (row board)
    (dolist (cell row)
      (format t "~2d " cell))
    (format t "~%")))

(defun print-kojun-regions (regions)
  (dolist (row regions)
    (dolist (cell row)
      (format t "~2d " cell))
    (format t "~%")))

;; Usage
(print-kojun-board (kojun-board))
